{
    "name": "nickwest/eloquent-forms",
    "description": "Create Bulma compatible forms and tables directly from Eloquent Models and Collections",
    "authors": [
        {
            "name": "Nick West",
            "email": "nickwest@uw.edu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "nesbot/carbon": "^1.23",
        "doctrine/dbal": "~2.6",
        "maatwebsite/excel": "^3.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Nickwest\\EloquentForms\\": "src/",
            "Nickwest\\EloquentForms\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.6",
        "orchestra/database": "~3.6",
        "laravel/laravel": "5.6.*",
        "sunra/php-simple-html-dom-parser": "^1.5"
        },
    "extra": {
        "laravel": {
            "providers": [
                "Nickwest\\EloquentForms\\EloquentFormsServiceProvider",
                "Nickwest\\EloquentForms\\Themes\\bulma\\EloquentFormsBulmaThemeServiceProvider"
            ]
        }
    }
}
